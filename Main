local Cursor=Exploiter:WaitForChild("PlayerGui"):FindFirstChild("Cursor")
local tab={}

local ExploitFolder=Instance.new("Folder",Exploiter)
ExploitFolder.Name=randomString()

local hitSound=Instance.new("Sound",ExploitFolder)
hitSound.Looped=false
hitSound.SoundId="rbxassetid://8679627751"
hitSound.Volume=0.6

local AnimR6=Instance.new("Animation",ExploitFolder)
local AnimR15=Instance.new("Animation",ExploitFolder)

AnimR6.AnimationId="rbxassetid://18350615991" --18350615991
AnimR15.AnimationId="rbxassetid://18345481463" --18345481463

local function tryFindPlr(Needs)
	if not Needs or typeof(Needs)~="string" then return end
	Needs=string.lower(Needs)
	local TableOfNames=Players:GetPlayers()

	for _,v in TableOfNames do
		local Name_a=string.lower(v.Name)
		if string.match(Name_a,Needs) then
			return v
		end
	end
end

local function checkRig()
	local Char=Exploiter.Character
	if Char:FindFirstChild("Torso") then
		return "r6"
	elseif Char:FindFirstChild("UpperTorso") then
		return "r15"
	else
		return "other"
	end
end

local function getRoot(Char)
	if Char then
		return Char.PrimaryPart or Char:FindFirstChild("Torso") or Char:FindFirstChild("UpperTorso")
	end
end

local function tpPos(X,Y,Z)
	local Root=getRoot(Exploiter.Character)
	if typeof(X or Y or Z)~="number" then warn("Ð½Ðµ ÑÐ¸ÑÐ»Ð¾") return end
	Root.CFrame=Root.CFrame*CFrame.new(X,Y,Z)
end

local function tpPlayer(PlayerName)
	local NowPlayer=tryFindPlr(PlayerName)
	if not NowPlayer then warn("Ð½Ðµ Ð½Ð°ÑÑÐ» Ð¸Ð³ÑÐ¾ÐºÐ°") return end
	local Chr=NowPlayer.Character

	local RootPlr=getRoot(Chr)
	if Chr and RootPlr then
		getRoot(Exploiter.Character).CFrame=RootPlr.CFrame*CFrame.new(0,0,2)
	end
end

local function setWalkSpeed(Number)
	if typeof(Number)~="number" then warn("Ð½Ðµ ÑÐ¸ÑÐ»Ð¾") return end

	if Thread and tonumber(Number)==0 then coroutine.close(Thread) return end
	if Thread then coroutine.close(Thread) end

	Thread=coroutine.create(function()
		while task.wait() do
			local Hum=nil
			if not Exploiter.Character:FindFirstChildOfClass("Humanoid") then continue end
			Hum=Exploiter.Character:FindFirstChildOfClass("Humanoid")

			Hum.WalkSpeed=Number
		end
	end)
	coroutine.resume(Thread)
end

local function drawESP()
	ThreadESP=coroutine.create(function()
		while task.wait(.5) do
			for _,v in Players:GetPlayers() do
				local Model=v.Character
				if not Model or Model:FindFirstChild("ESP") or Model==Exploiter.Character or not Model.PrimaryPart then continue end
				
				local Root=getRoot(Model)
				
				local Bild=Instance.new("BillboardGui",Model)
				local Frame=Instance.new("Frame",Bild)
				local UIs=Instance.new("UIStroke",Frame)

				Bild.Name="ESP"
				Bild.Size=UDim2.new(4,0,6,0)
				Bild.Adornee=Root
				Bild.AlwaysOnTop=true

				Frame.BackgroundTransparency=1
				Frame.AnchorPoint=Vector2.new(0.5,0.5)
				Frame.Size=UDim2.new(1,0,1,0)
				Frame.Position=UDim2.new(0.5,0,0.5,0)

				UIs.Color=Color3.fromRGB(0, 255, 0)
				UIs.Thickness=1.5

				local Bild2=Instance.new("BillboardGui",Model)
				local Frame2=Instance.new("Frame",Bild2)
				local TextLabel=Instance.new("TextLabel",Frame2)

				Bild2.Name="ESP2"
				Bild2.Size=UDim2.new(4,0,12,0)
				Bild2.StudsOffset=Vector3.new(0,0,-1)
				Bild2.Adornee=Root
				Bild2.AlwaysOnTop=true

				Frame2.Name="Frame2"
				Frame2.BackgroundTransparency=1
				Frame2.AnchorPoint=Vector2.new(0.5,0.5)
				Frame2.Size=UDim2.new(1,0,0.25,0)
				Frame2.Position=UDim2.new(0.5,0,0,0)

				TextLabel.BackgroundTransparency=1
				TextLabel.AnchorPoint=Vector2.new(0.5,0.5)
				TextLabel.Size=UDim2.new(1,0,1,0) 
				TextLabel.Position=UDim2.new(0.5,0,0.5,0)
				TextLabel.TextColor3=Color3.fromRGB(255, 255, 255)
				TextLabel.TextStrokeColor3=Color3.fromRGB(0, 0, 0)
				TextLabel.TextSize=7
				TextLabel.TextStrokeTransparency=0
				TextLabel.Text=Model.Name
			end
		end
	end)
	coroutine.resume(ThreadESP)
end

local function destoyESP()
	if ThreadESP then
		coroutine.close(ThreadESP)
		ThreadESP=nil
	end

	for _,v in Players:GetPlayers() do
		local Model=v.Character
		if not Model or not Model:FindFirstChild("ESP") or not Model:FindFirstChild("ESP2") or not getRoot(Model) then continue end

		Model.ESP:Destroy()
		Model.ESP2:Destroy()
	end
end

local function updateInput(input)
	local delta=input.Position-dragStart
	local position=UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	MainFrame.Position=position
end
--Code

Modal.Modal=not Modal.Modal
MainFrame.Visible=true
State=not State

local function toggleButtonColor(Button,Mode)
	if not Button then return end
	if Mode then
		TweenService:Create(Button,TweenInfo.new(.25),{BackgroundColor3=Color3.fromRGB(187, 153, 221)}):Play()
	else
		TweenService:Create(Button,TweenInfo.new(.25),{BackgroundColor3=Color3.fromRGB(79, 79, 79)}):Play()
	end
end

UserInputService.InputBegan:Connect(function(input,gpe)
	local int=input.KeyCode
	if int==Enum.KeyCode.Insert and not gpe then
		Modal.Modal=not Modal.Modal
		Main2.Visible=not Main2.Visible
		if State then MainFrame.Visible=true else MainFrame.Visible=false end
		State=not State
	elseif int==Enum.KeyCode.PageDown and not gpe then
		MainZHUB:Destroy()
		Cursor:Destroy()
		AimBound:Destroy()
		destoyESP()
		_G.ZHub_Loaded=nil
		RegButton=nil
		script:Destroy()
	end
end)
local Instructions={
	["TpCordsBut"]=function()
		tpPos(tonumber(X.Text),tonumber(Y.Text),tonumber(Z.Text))
	end,
	["TpPlrBut"]=function()
		tpPlayer(Player.Text)
	end,
	["SetSpeedBut"]=function()
		setWalkSpeed(tonumber(SpeedNum.Text))
	end,
	["EnableAimbot"]=function()
		local C=nil

		table.clear(tab)
		
		toggleButtonColor(EnableAimbot,false)

		if Aimbot then
			RunService:UnbindFromRenderStep("Aimbot")
			table.clear(tab)
			Aimbot=false
			return
		end

		for _,Plr in Players:GetPlayers() do
			local Vec2P,Vec2M,Pos,OnScreen=nil
			local CRoot=getRoot(Plr.Character)
			
			if Plr.Character==Exploiter.Character then continue end
			C=Plr.Character
			if not C or not CRoot then continue end

			Pos,OnScreen=Camera:WorldToScreenPoint(CRoot.Position)
			Vec2P,Vec2M=Vector2.new(Pos.X,Pos.Y),Vector2.new(Mouse.X,Mouse.Y)

			if OnScreen==true then
				table.insert(tab,{C,(Vec2P-Vec2M).Magnitude})
			end
		end

		local min=math.huge
		C=nil
		if #tab>0 then
			for _,v in tab do
				if v[2]<min and v[2]<=tonumber(SetFOV.Text) then min=v[2] end
			end
			for _,v in tab  do
				if v[2]==min then
					C=v[1]
					break
				end
			end
		end
		
		local CRoot=getRoot(C)
		if not C or not CRoot or #tab==0 then return end
		local Head=C:FindFirstChild("Head")		
		if not Head then
			if CRoot then
				Aimbot=true

				RunService:BindToRenderStep("Aimbot", Enum.RenderPriority.Camera.Value-1, function()
					Camera.CFrame=CFrame.new(Camera.CFrame.Position,CRoot)
					
					HealthLast=C.Humanoid.Health
					local conn
					conn=C.Humanoid:GetPropertyChangedSignal("Health"):Connect(function()
						HealthNow=C.Humanoid.Health
						for i,v in C.Humanoid:GetChildren() do
							if v:IsA("ObjectValue") and v.Value and v.Value:IsA("Player") then
								local kill=v.Value
								if kill==Exploiter then
									hitSound:Play()
									conn:Disconnect()
									
									HealthLast=HealthNow
									return
								end
							end
						end
						return
					end)
				end)

				toggleButtonColor(EnableAimbot,true)
			else
				return
			end
		else
			Aimbot=true

			RunService:BindToRenderStep("Aimbot", Enum.RenderPriority.Camera.Value-1, function()
				Camera.CFrame=CFrame.new(Camera.CFrame.Position,Head.Position)
				
				HealthLast=C.Humanoid.Health
				local conn
				conn=C.Humanoid:GetPropertyChangedSignal("Health"):Connect(function()
					HealthNow=C.Humanoid.Health
					for i,v in C.Humanoid:GetChildren() do
						if v:IsA("ObjectValue") and v.Value and v.Value:IsA("Player") then
							local kill=v.Value
							if kill==Exploiter and HealthNow<HealthLast then
								hitSound:Play()
								conn:Disconnect()
								
								HealthLast=HealthNow
								return
							end
						end
					end
					return
				end)
			end)

			toggleButtonColor(EnableAimbot,true)
		end
	end,
	["EnableESP"]=function()
		if ESPstate==false then 
			toggleButtonColor(EnableESP,true)
			drawESP() 
		else 
			toggleButtonColor(EnableESP,false)
			destoyESP()
		end
		ESPstate=not ESPstate
	end,
	["EnableAimfov"]=function()
		if Circle.Visible==false then
			toggleButtonColor(EnableAimfov,true)
		else
			toggleButtonColor(EnableAimfov,false)
		end
		Circle.Visible=not Circle.Visible
	end,
	["EnableAntiaim"]=function()
		toggleButtonColor(EnableAntiaim,false)
		
		local track
		if Exploiter.Character and Exploiter.Character:FindFirstChild("Humanoid") then
			if checkRig()=="r15" then
				track=Exploiter.Character.Humanoid.Animator:LoadAnimation(AnimR15)
			else
				track=Exploiter.Character.Humanoid.Animator:LoadAnimation(AnimR6)
			end
		end
		if not track then return end
		
		if AntiAim_a then
			for i,v in getRoot(Exploiter.Character):GetChildren() do
				if v:IsA("BodyAngularVelocity") then
					v:Destroy()
				end
			end
			AntiAim_a=false
			track:Stop()
			track=nil
			if ThreadAntiAim then
				coroutine.close(ThreadAntiAim)
			end
			return
		end
		AntiAim_a=true
		track:Play()

		local Anti=Instance.new("BodyAngularVelocity")
		Anti.Parent=getRoot(Exploiter.Character)
		Anti.AngularVelocity=Vector3.new(0,20,0)
		Anti.MaxTorque=Vector3.new(0,math.huge,0)
		Anti.P = math.huge
		local Char=Exploiter.Character:GetChildren()
		for i,v in Char do
			if v:IsA("BasePart") then
				v.Velocity=Vector3.new(0, 0, 0)
			end
		end
		
		ThreadAntiAim=coroutine.create(function()
			while task.wait(1) do
				if AntiAim_a and getRoot(Exploiter.Character) and not getRoot(Exploiter.Character):FindFirstChildWhichIsA("BodyAngularVelocity") then
					if Exploiter.Character and Exploiter.Character:FindFirstChild("Humanoid") then
						if checkRig()=="r15" then
							track=Exploiter.Character.Humanoid.Animator:LoadAnimation(AnimR15)
						else
							track=Exploiter.Character.Humanoid.Anumator:LoadAnimation(AnimR6)
						end
					end
					if not track then continue end
					track:Play()

					local Anti=Instance.new("BodyAngularVelocity")
					Anti.Parent=getRoot(Exploiter.Character)
					Anti.AngularVelocity=Vector3.new(0,20,0)
					Anti.MaxTorque=Vector3.new(0,math.huge,0)
					Anti.P = math.huge
					local Char=Exploiter.Character:GetChildren()
					for i, v in next, Char do
						if v:IsA("BasePart") then
							v.Velocity=Vector3.new(0, 0, 0)
						end
					end
				end
			end
		end)
		coroutine.resume(ThreadAntiAim)
		
		toggleButtonColor(EnableAntiaim,true)
	end,
	["EnableTriggerbot"]=function()
		toggleButtonColor(EnableTriggerbot,false)

		if TriggerBot then
			RunService:UnbindFromRenderStep("TriggerBot")
			TriggerBot=false
			return
		end

		TriggerBot=true
		RunService:BindToRenderStep("TriggerBot",Enum.RenderPriority.Camera.Value-2,function()
			if TriggerBot_Aim then
				if Aimbot then
					local C
					if not Mouse.Target then return end
					local Model=Mouse.Target:FindFirstAncestorOfClass("Model")
					if Model then C=Model:FindFirstChildOfClass("Humanoid") end
					if C and tryFindPlr(Model.Name) then
						mouse1press() 
						mouse1release()
						--[[local conn
						conn=C:GetPropertyChangedSignal("Health"):Connect(function()
							hitSound:Play()
							conn:Disconnect()
							C=nil
							return
						end)]]
					end
				end
			else
				local C
				if not Mouse.Target then return end
				local Model=Mouse.Target:FindFirstAncestorOfClass("Model")
				if Model then C=Model:FindFirstChildOfClass("Humanoid") end
				if C and tryFindPlr(Model.Name) then
					mouse1press()
					mouse1release()
					--[[local conn
					conn=C:GetPropertyChangedSignal("Health"):Connect(function()
						hitSound:Play()
						conn:Disconnect()
						C=nil
						return
					end)]]
				end
			end
		end)

		toggleButtonColor(EnableTriggerbot,true)
	end,
	["EnableAimmode"]=function()
		if TriggerBot_Aim==false then
			toggleButtonColor(EnableAimmode,true)
		else
			toggleButtonColor(EnableAimmode,false)
		end
		TriggerBot_Aim=not TriggerBot_Aim
	end,
}

local Dones={EnableAimbot,EnableAimfov,EnableESP,EnableAimmode,EnableNametags,EnableCheckTeam,EnableTriggerbot,EnableAntiaim,SetSpeedBut,TpPlrBut,TpCordsBut}
for _,Button in Dones do
	Button.MouseButton1Click:Connect(function()
		Instructions[Button.Name]()
	end)
end

local MenuNow="RageMenu"
local MenuLast=nil

local MenusB=Menus:GetChildren()
for _,Button in MenusB do
	Button.MouseButton1Click:Connect(function()
		MenuLast=MenuNow
		MenuNow=Button.Name
		
		MainInside[MenuNow:sub(0,-5)].Visible=true
		
		TweenService:Create(Menus[MenuNow],TweenInfo.new(.4),{BackgroundTransparency=0}):Play()
		
		if MenuLast~=MenuNow then
			TweenService:Create(Menus[MenuLast],TweenInfo.new(.4),{BackgroundTransparency=1}):Play()
			MainInside[MenuLast:sub(0,-5)].Visible=false
		end
	end)
end

Bind.MouseButton1Click:Connect(function()
	Bind.Text="Rec..."
	Bind.TextColor3=Color3.fromRGB(148, 148, 148)

	local conn
	conn=UserInputService.InputBegan:Connect(function(input)
		RegButton=input.KeyCode
		if RegButton==Enum.KeyCode.Escape then
			RegButton=Enum.KeyCode.X
		end

		Bind.Text=RegButton.Name
		Bind.TextColor3=Color3.fromRGB(187, 153, 221)
		conn:Disconnect()
	end)
end)

UserInputService.InputBegan:Connect(function(input,gpe)
	if RegButton and input.KeyCode==RegButton and not gpe then
		Instructions[EnableAimbot.Name]()
	end
end)

local SettingsTable={Settings,Settings_2,Settings_3}
for _,Button in SettingsTable do
	Button.MouseButton1Click:Connect(function()
		local Needed=Button.Parent.Name.."Settings"
		
		for _,v in Button.Parent.Parent.Parent.InOutside:GetChildren() do
			v.Visible=false
		end
		
		Button.Parent.Parent.Parent.InOutside[Needed].Visible=true
	end)
end

local Mouse=coroutine.create(function()
	while task.wait() do
		Main2.Position=UDim2.new(0,Mouse.X,0,Mouse.Y)
		Circle.Position=UDim2.new(0,Mouse.X,0,Mouse.Y)
	end
end)
coroutine.resume(Mouse)

SetFOV:GetPropertyChangedSignal("Text"):Connect(function()
	if typeof(tonumber(SetFOV.Text))=="number" and SetFOV.Text~="" then
		Circle.Size=UDim2.new(0,tonumber(SetFOV.Text)*2,0,tonumber(SetFOV.Text)*2)
	end
end)

Drag.InputBegan:Connect(function(input)
	if input.UserInputType==Enum.UserInputType.MouseButton1 then 
		dragToggle=true
		dragStart=input.Position
		startPos=MainFrame.Position
		input.Changed:Connect(function()
			if input.UserInputState==Enum.UserInputState.End then
				dragToggle=false
			end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input.UserInputType==Enum.UserInputType.MouseMovement then
		if dragToggle then
			updateInput(input)
		end
	end
end)

_G.ZHub_Loaded=true
