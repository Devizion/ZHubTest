function randomString()
	local length=math.random(10,20)
	local array={}
	for i=1, length do
		array[i]=string.char(math.random(32,126))
	end
	return table.concat(array)
end
--step1
local CompleteAuto=false
do
	local path="https://zhub-788fb-default-rtdb.europe-west1.firebasedatabase.app/Accounts"

	local reqTotal=game:HttpGet(path..".json")

	local SearchTable=HttpService:JSONDecode(reqTotal)
	for i,v in SearchTable do
		if v==nil then
			table.remove(SearchTable,i)
		end
	end

	for _,v in SearchTable do
		if typeof(v)=="table" and v["UID"]==Exploiter.UserId then
			CompleteAuto=true
		end
	end
	if CompleteAuto==false then
		while true do
			if true then else end
		end
	end
end

